                    <h3 class="title">About</h3>
                    <!-- Slide Text -->
                    <h3 class="subtitle">The Academy of Majapahit Martial Arts is a school that specializes in the martial methodologies of Southeast Asia</h3>
                    <!-- Slide Image -->
                    <img class="slide-img" src="~/Content/img/MAJ_majapahit_empire_map.png" alt="majapahit" />
                </li>
                <!-- End Slide 3 -->
            </ul>
            <div class="sequence-pagination-wrapper">
                <ul class="sequence-pagination">
                    <li>1</li>
                    <li>2</li>
                    <li>3</li>
                </ul>
            </div>
        </div>
    </div>
